# Server Configuration
PORT=3000
NODE_ENV=development
LOG_LEVEL=info

# CORS Configuration
CORS_ORIGIN=*

# Rate limiting
RATE_LIMIT_WINDOW_MS=15000
RATE_LIMIT_MAX_REQUESTS=100

# Download settings
MAX_VIDEO_SIZE_MB=500
DOWNLOAD_TIMEOUT_SECONDS=300

# Cleanup settings
AUTO_CLEANUP_ENABLED=true
CLEANUP_AGE_DAYS=7


